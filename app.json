{
  "expo": {
    "name": "bookbeauty-native",
    "slug": "bookbeauty-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bookbeautynative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "softwareKeyboardLayoutMode": "resize",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "BookBeauty heeft toegang nodig tot je foto's om logo's en video's te uploaden.",
          "cameraPermission": "BookBeauty heeft toegang nodig tot je camera om media te maken.",
          "microphonePermission": "BookBeauty heeft toegang nodig tot je microfoon bij video-opnames."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "EXPO_PUBLIC_CLOUDINARY_CLOUD_NAME": "djrw0k1fy",
      "EXPO_PUBLIC_CLOUDINARY_UPLOAD_PRESET": "bookbeauty_unsigned"
    }
  }
}
